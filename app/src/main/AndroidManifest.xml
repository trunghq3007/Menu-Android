<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.menu">

    <application
        android:allowBackup="true"
        android:icon="@drawable/menuicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/menuicon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ToolbarActivity"
            android:label="@string/title_activity_toolbar"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".OptionMenuActivity"
            android:label="@string/title_activity_option_menu"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ContextMenuActivity" />
        <activity
            android:name=".Main2Activity"
            android:label="@string/title_activity_main2"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>